<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="myapp.dbd_box.dao.OfferingDao">

    <select id="getAddonListByKillerId" resultType="Offering">
        select * from offering where id = #{id}
    </select>

    <select id="getOfferingListByClass" resultType="Offering">
        select * from offering where class_name = #{class_name}
    </select>

    <select id="getOfferingListByBelong" resultType="Offering">
        select * from offering where belong = #{id}
    </select>

</mapper>